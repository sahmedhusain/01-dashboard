# Server Configuration
PORT=8080
HOST=localhost
ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRATION=24h

# API URLs
AUTH_API_URL=https://learn.reboot01.com/api/auth
GRAPHQL_API_URL=https://learn.reboot01.com/api/graphql-engine/v1/graphql

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT=100
RATE_LIMIT_DURATION=1m

# Cache Settings
CACHE_TTL=5m
CACHE_SIZE=1000
